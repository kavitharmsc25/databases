create database kavitha_db;
use kavitha_db;
create table employees(
    empid int,
    empname varchar(50),
    empage int,
    dept varchar(50),
    salary double
);

insert into employees values(101, "abi", 23, "velachery", "php developer", 500000),
(102, "naya", 23, "palikarainai", "react developer", 60000);
 select * from employees where name="abi";


